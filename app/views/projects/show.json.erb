{
    "id" : "<%= @project.id %>",
    "status" : "<%= @project.dashboard.status unless @project.dashboard.nil?%>",
    <% if @project.dashboard && @project.dashboard.complete?  %>
      "output" : {
        "key" : "<%= @project.dashboard.outputs.first.key%>",
        "value" : "<%= @project.dashboard.outputs.first.value%>"
      },
    <% end %>
    "name" : "<%= @project.name %>"
}
