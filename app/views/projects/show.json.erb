{
    "id" : "<%= @project.id %>",
    "status" : "<%= @project.dashboard.status unless @project.dashboard.nil?%>",
    <% if @project.dashboard && @project.dashboard.status == :running  %>
      "output" : {
        "aws_dns_name" : "<%= @project.dashboard.dns_name%>",
        "key" : "url",
        "value" : "<%= @project.dashboard_url%>"
      },
    <% end %>
    "name" : "<%= @project.name %>"
}
